<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.liferay.screens">
	<author>Jos√© Manuel Navarro</author>
	<namespace>Screens</namespace>
	<entity name="ScreensAssetEntry" local-service="false" remote-service="true">
		<reference package-path="com.liferay.portal" entity="ClassName" />
		<reference package-path="com.liferay.portlet.blogs" entity="BlogsEntry" />
		<reference entity="DDLRecord" package-path="com.liferay.portlet.dynamicdatalists" />
		<reference entity="DDLRecordSet" package-path="com.liferay.portlet.dynamicdatalists" />
		<reference entity="DDMStructure" package-path="com.liferay.portlet.dynamicdatamapping" />
		<reference entity="JournalArticle" package-path="com.liferay.portlet.journal" />
		<reference entity="JournalArticleResource" package-path="com.liferay.portlet.journal" />
		<reference entity="Layout" package-path="com.liferay.portal" />
		<reference entity="PortletItem" package-path="com.liferay.portal" />
		<reference entity="PortletPreferences" package-path="com.liferay.portal" />
		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset" />
		<reference entity="DLApp" package-path="com.liferay.portlet.documentlibrary" />
		<reference entity="DLFileEntry" package-path="com.liferay.portlet.documentlibrary" />
	</entity>
	<entity local-service="false" name="ScreensComment" remote-service="true">
		<reference package-path="com.liferay.portal" entity="ClassName" />
		<reference entity="Group" package-path="com.liferay.portal" />
		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset" />
		<reference entity="MBMessage" package-path="com.liferay.portlet.messageboards" />
		<reference entity="MBDiscussion" package-path="com.liferay.portlet.messageboards"/>
	</entity>
	<entity name="ScreensDDLRecord" local-service="false" remote-service="true">
		<reference package-path="com.liferay.portlet.dynamicdatalists" entity="DDLRecord" />
	</entity>
	<entity name="ScreensJournalArticle" local-service="false" remote-service="true">
		<reference package-path="com.liferay.portlet.journal" entity="JournalArticle" />
		<reference package-path="com.liferay.portlet.journal" entity="JournalArticleResource" />
	</entity>
	<entity local-service="false" name="ScreensRatingsEntry" remote-service="true">
		<reference entity="AssetEntry" package-path="com.liferay.portlet.asset" />
		<reference entity="RatingsEntry" package-path="com.liferay.portlet.ratings" />
	</entity>
	<entity name="ScreensUser" local-service="false" remote-service="true">
		<reference package-path="com.liferay.portal" entity="Company" />
	</entity>
</service-builder>